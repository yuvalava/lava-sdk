{
  "name": "lava-sdk",
  "version": "1.0.0",
  "description": "An SDK for interacting with Lava provider",
  "main": "main.ts",
  "author": "Lava Protocol Inc",
  "license": "Apache-2.0",
  "scripts": {
    "tsc": "tsc",
    "build": "yarn tsc && yarn chmod && webpack --config webpack.config.js",
    "chmod": "chmod u+x ./bin/main.js",
    "protoc": "./scripts/protoc.sh",
    "server": "http-server dist/ -c-1",
    "test": "jest ./src",
    "example": "node ./bin/examples/basic.js",
    "prettier": "prettier --check .",
    "lint": "eslint '**/*.ts'"
  },
  "bin": {
    "lava-sdk": "./bin/main.js"
  },
  "devDependencies": {
    "@types/jest": "^29.2.2",
    "http-server": "^14.1.1",
    "typescript": "^4.8.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "@cosmjs/amino": "^0.29.4",
    "@cosmjs/crypto": "^0.29.4",
    "@cosmjs/encoding": "^0.29.4",
    "@cosmjs/proto-signing": "^0.29.4",
    "@cosmjs/stargate": "0.28.2",
    "@grpc/grpc-js": "^1.7.1",
    "@improbable-eng/grpc-web": "^0.15.0",
    "@improbable-eng/grpc-web-node-http-transport": "^0.15.0",
    "@types/google-protobuf": "^3.15.6",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "chalk": "4.1.2",
    "commander": "^9.4.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "google-protobuf": "^3.21.2",
    "grpc-web": "^1.4.2",
    "jest": "^29.3.1",
    "long": "^5.2.1",
    "protobufjs": "^7.1.2",
    "rxjs": "^7.5.7",
    "ts-jest": "^29.0.3",
    "ts-protoc-gen": "^0.15.0"
  }
